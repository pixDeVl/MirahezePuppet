[mysqld.<%= @title %>]
datadir = <%= @datadir %>
tmpdir = <%= @tmpdir %>
socket = <%= @socket %>
port = <%= @port %>
extra-port = <%= Integer(@port) + 20 %>
log-error = <%= @error_log %>
<% if @innodb_buffer_pool_size -%>
innodb_buffer_pool_size = <%= @innodb_buffer_pool_size %>
<% end -%>
read-only = <%= @read_only %>
pid-file = <%= @pid_file %>
ssl
ssl-capath                     = /etc/ssl/certs
ssl-cert                       = /etc/ssl/localcerts/wikitide.net.crt
ssl-key                        = /etc/ssl/private/wikitide.net.key
